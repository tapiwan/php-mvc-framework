[
  {
    "id": "application",
    "class": "bitbetrieb\\CMS\\Application\\Application",
    "type": "singleton",
    "dependencies": [
      "front-controller"
    ]
  },
  {
    "id": "front-controller",
    "class": "bitbetrieb\\CMS\\FrontController\\FrontController",
    "type": "singleton",
    "dependencies": [
      "request",
      "routes-php"
    ]
  },
  {
    "id": "config",
    "class": "bitbetrieb\\CMS\\Config\\Config",
    "type": "singleton",
    "dependencies": [
      "config-file"
    ]
  },
  {
    "id": "config-file",
    "value": "../config/config.json",
    "type": "value"
  },
  {
    "id": "request",
    "class": "bitbetrieb\\CMS\\HTTP\\Request",
    "type": "singleton",
    "dependencies": []
  },
  {
    "id": "routes-php",
    "value": "../app/routes.php",
    "type": "value"
  },
  {
    "id": "controller-namespace",
    "value": "\\bitbetrieb\\CMS\\Controller\\",
    "type": "value"
  },
  {
    "id": "models-namespace",
    "value": "\\bitbetrieb\\CMS\\Model\\",
    "type": "value"
  }
]
