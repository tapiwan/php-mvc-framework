[
  {
    "id": "application",
    "class": "bitbetrieb\\MVC\\Application\\Application",
    "type": "singleton",
    "dependencies": [
      "config",
      "front-controller"
    ]
  },
  {
    "id": "config",
    "class": "bitbetrieb\\MVC\\Config\\Config",
    "type": "singleton",
    "dependencies": [
      "config-file"
    ]
  },
  {
    "id": "config-file",
    "value": "../config/config.json",
    "type": "value"
  },
  {
    "id": "front-controller",
    "class": "bitbetrieb\\MVC\\FrontController\\FrontController",
    "type": "singleton",
    "dependencies": [
      "request"
    ]
  },
  {
    "id": "request",
    "class": "bitbetrieb\\MVC\\HTTP\\Request",
    "type": "singleton",
    "dependencies": []
  },
  {
    "id": "database-handler",
    "class": "bitbetrieb\\MVC\\DatabaseHandler\\DatabaseHandler",
    "type": "singleton",
    "dependencies": [
      "config"
    ]
  }
]
