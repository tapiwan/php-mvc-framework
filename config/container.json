[
  {
    "id": "application",
    "class": "bitbetrieb\\CMS\\Application\\Application",
    "type": "singleton",
    "dependencies": [
      "$this",
      "config",
      "front-controller"
    ]
  },
  {
    "id": "front-controller",
    "class": "bitbetrieb\\CMS\\FrontController\\FrontController",
    "type": "singleton",
    "dependencies": [
      "request",
      "response",
      "routes-file"
    ]
  },
  {
    "id": "request",
    "class": "bitbetrieb\\CMS\\HTTP\\Request",
    "type": "singleton",
    "dependencies": []
  },
  {
    "id": "response",
    "class": "bitbetrieb\\CMS\\HTTP\\Response",
    "type": "singleton",
    "dependencies": []
  },
  {
    "id": "routes-file",
    "value": "../app/routes.php",
    "type": "value"
  },
  {
    "id": "config",
    "class": "bitbetrieb\\CMS\\Config\\Config",
    "type": "singleton",
    "dependencies": [
      "config-file"
    ]
  },
  {
    "id": "config-file",
    "value": "../config/config.json",
    "type": "value"
  },
  {
    "id": "controller-namespace",
    "value": "\\bitbetrieb\\CMS\\Controller\\",
    "type": "value"
  },
  {
    "id": "models-namespace",
    "value": "\\bitbetrieb\\CMS\\Model\\",
    "type": "value"
  },
  {
    "id": "view-directory",
    "value": "../resources/views/",
    "type": "value"
  },
  {
    "id": "database-handler",
    "class": "bitbetrieb\\CMS\\DatabaseHandler\\DatabaseHandler",
    "type": "singleton",
    "dependencies": [
      "config"
    ]
  }
]
